    <!-- javascript -->
    <script>
        document.getElementById("bookAppo").addEventListener("click", function () {
            document.querySelector(".registration-form").style.display = "block";
        })

        document.getElementById("closeX").addEventListener("click",
            function () {
                document.querySelector(".registration-form").style.display = "none";
            })


        //review
        document.getElementById("writeReview").addEventListener("click", function () {
            document.querySelector(".reviewPopup").style.display = "block";
        })
        document.getElementById("clsX").addEventListener("click", function () {
            document.querySelector(".reviewPopup").style.display = "none";
        })

        // submit review
        document.getElementById("saveReview").addEventListener("click", function () {
            document.querySelector(".review-form").style.display = "block";
        })
        document.getElementById("Xclse").addEventListener("click", function () {
            document.querySelector(".review-form").style.display = "none";
